{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "cache": "localhost:6379,abortConnect=false,AllowAdmin=true",
    "messaging": "amqp://admin:secret@localhost:5672",
    "db": "Host=127.0.0.1;Port=5432;Database=trace;Username=trace;Password=trace;Timeout=15;Pooling=true;MinPoolSize=1;MaxPoolSize=20;ConnectionLifeTime=15"
  },
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 5
        },
        "HealthChecks": false,
        "Tracing": true
      }
    }
  }
}
